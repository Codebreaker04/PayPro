{
  "version": 2,
  "name": "PayPro App",
  "builds": [
    {
      "src":  "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "api/v1/(.*)",
      "dest": "/",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
        ]
    }
  ]
}